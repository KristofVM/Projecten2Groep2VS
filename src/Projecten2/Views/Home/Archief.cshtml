@{
    ViewData["Title"] = "Overzicht";
}

<main>
    <div class="contain archief">
        <h1>Archief</h1>
        <div class="archiefbuttons">
            <button class="undo"><i class="fa fa-mail-reply"></i></button>
            <button class="zoek"><i class="fa fa-search"></i></button>
            <input type="text" name="zoekanalyse" class="zoekbalk" placeholder="tik om te soek...">
        </div>

        <div class="archieflijst">
            <div class="archiefbalk">
                <div class="checktop"></div>
                <span class="anaam">Naam</span>
                <span class="abedrijf">Bedrijf</span>
                <span class="aafdeling">Afdeling</span>
                <span class="adatum">Datum</span>
                <span class="aomzet">Omzet</span>
                <div class="clear"></div>
            </div>
            <div class="archiefanalyse">
                <input type="checkbox" name="selectanalyse" value="" class="check">
                <span class="anaam">Analyse1</span>
                <span class="abedrijf">Google</span>
                <span class="aafdeling">Kuisdienst</span>
                <span class="adatum">17/02/2017</span>
                <span class="aomzet omzetanalyse">0</span>
                <div class="clear"></div>
            </div>
            <div class="archiefanalyse">
                <input type="checkbox" name="selectanalyse" value="" class="check">
                <span class="anaam">Analyse2</span>
                <span class="abedrijf">Google</span>
                <span class="aafdeling">Webdesign</span>
                <span class="adatum">17/02/2017</span>
                <span class="aomzet omzetanalyse">0</span>
                <div class="clear"></div>
            </div>
            <div class="archiefanalyse">
                <input type="checkbox" name="selectanalyse" value="" class="check">
                <span class="anaam">Analyse3</span>
                <span class="abedrijf">Van Den Nest</span>
                <span class="aafdeling">Verkoop</span>
                <span class="adatum">17/02/2017</span>
                <span class="aomzet omzetanalyse">0</span>
                <div class="clear"></div>
            </div>
            <div class="archiefanalyse">
                <input type="checkbox" name="selectanalyse" value="" class="check">
                <span class="anaam">Analyse4</span>
                <span class="abedrijf">Beenhouwerij Renmans</span>
                <span class="aafdeling">Beenhouwerij1</span>
                <span class="adatum">17/02/2017</span>
                <span class="aomzet omzetanalyse">0</span>
                <div class="clear"></div>
            </div>
        </div>
    </div>
</main>