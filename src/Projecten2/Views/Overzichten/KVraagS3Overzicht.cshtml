@model Projecten2.Models.Domain.Analyse


<main class="overzicht">
    <div class="contain">
        <h1>Welke extra kosten moet u structureel extra maken voor deze inhuur? (alleen kosten zoals werkkleding bij normale inhuur en bij reshoring alle kosten)</h1>
        <a asp-controller="Vragen" asp-action="KVraagS4" asp-route-AnalyseId="@Model.AnalyseId" asp-route-VraagId="3" class="terug button"><i class="fa fa-plus"></i> Nieuwe type</a>
        <table style="width:30%">
            <tr>
                <th>Type</th>
                <th>Bedrag</th>
            </tr>
            @if (Model.Kosten.Kvragen3.Count == 0)
            {
                <tr>
                    <th colspan="2" class="alert">Er zijn geen functies.</th>
                </tr>
            }
            @foreach (var item in Model.Kosten.Kvragen3)
            {
                <tr>
                    <th>@item.Type</th>
                    <th>@item.Bedrag</th>
                </tr>
            }
        </table>
    </div>
</main>