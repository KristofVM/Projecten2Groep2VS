@model Projecten2.Models.Domain.Analyse


<main class="overzicht">
    <div class="contain">
        <h1>Welke kosten moet u ter voorbereiding maken om dit te implementeren?</h1>
        <a asp-controller="Vragen" asp-action="BVraagS3" asp-route-AnalyseId="@Model.AnalyseId" class="terug button"><i class="fa fa-plus"></i> Nieuwe type</a>
        <table style="width:30%">
            <tr>
                <th>Type besparing</th>
                <th>Jaarbedrag</th>
            </tr>
            @if (Model.Baten.Bvragen11.Count == 0)
            {
                <tr>
                    <th colspan="2" class="alert">Er zijn geen functies.</th>
                </tr>
            }
            @foreach (var item in Model.Baten.Bvragen11)
            {
                <tr>
                    <th>@item.TypeBesparing</th>
                    <th>@item.JaarBedrag</th>
                </tr>
            }
        </table>
    </div>
</main>